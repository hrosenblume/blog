{
  "name": "blog-js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "prisma generate && rm -rf .next && next dev",
    "dev:tunnel": "bash scripts/dev-tunnel.sh",
    "dev:tunnel:open": "bash scripts/dev-tunnel.sh --open",
    "dev:mobile": "npm run dev:tunnel",
    "build": "prisma generate && next build",
    "build:prod": "prisma generate --schema=prisma/schema.postgresql.prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:migrate": "export $(grep -v '^#' .env.local | xargs) && prisma migrate dev",
    "db:migrate:status": "export $(grep -v '^#' .env.local | xargs) && prisma migrate status",
    "db:push": "export $(grep -v '^#' .env.local | xargs) && prisma db push",
    "db:push:prod": "export $(grep -v '^#' .env.local | xargs) && prisma db push --schema=prisma/schema.postgresql.prisma",
    "db:studio": "export $(grep -v '^#' .env.local | xargs) && prisma studio",
    "db:studio:prod": "export $(grep -v '^#' .env.local | xargs) && prisma studio --schema=prisma/schema.postgresql.prisma",
    "db:seed": "npx tsx --env-file=.env.local prisma/seed.ts",
    "db:seed:prod": "npx tsx --env-file=.env.local prisma/seed.ts --schema=prisma/schema.postgresql.prisma",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "db:setup:prod": "npm run db:push:prod && npm run db:seed:prod",
    "db:export": "npx tsx --env-file=.env.local scripts/export-data.ts",
    "db:import:prod": "npx tsx --env-file=.env.local scripts/import-data.ts",
    "build:thumbnails": "node scripts/polyhedra/build-thumbnails.js",
    "build:placeholders": "node scripts/polyhedra/build-placeholders.js",
    "auto-draft": "npx tsx --env-file=.env.local scripts/auto-draft.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-s3": "^3.948.0",
    "@mozilla/readability": "^0.6.0",
    "@prisma/client": "^4.16.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@tiptap/extension-bubble-menu": "^3.11.1",
    "@tiptap/extension-image": "^3.11.1",
    "@tiptap/extension-link": "^3.11.1",
    "@tiptap/extension-placeholder": "^3.11.1",
    "@tiptap/pm": "^3.11.1",
    "@tiptap/react": "^3.11.1",
    "@tiptap/starter-kit": "^3.11.1",
    "@types/turndown": "^5.0.6",
    "autoblogger": "file:../autoblogger",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "jsdom": "^27.4.0",
    "lucide-react": "^0.555.0",
    "marked": "^9.0.0",
    "next": "^15.5.9",
    "next-auth": "^5.0.0-beta.25",
    "next-seo": "^7.0.1",
    "next-themes": "^0.4.6",
    "openai": "^6.10.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-page-tracker": "^0.5.0",
    "rss-parser": "^3.13.0",
    "sanitize-html": "^2.17.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "turndown": "^7.2.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/sanitize-html": "^2.16.0",
    "autoprefixer": "^10.4.0",
    "better-sqlite3": "^12.4.6",
    "canvas": "^3.2.0",
    "ngrok": "^5.0.0-beta.2",
    "postcss": "^8.4.0",
    "prisma": "^4.16.2",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  }
}
