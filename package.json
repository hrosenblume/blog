{
  "name": "blog-js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "export $(grep -v '^#' .env.local | xargs) && prisma db push",
    "db:studio": "export $(grep -v '^#' .env.local | xargs) && prisma studio",
    "db:seed": "npx tsx --env-file=.env.local prisma/seed.ts",
    "db:setup": "npm run db:push && npm run db:seed"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "@tiptap/extension-bubble-menu": "^3.11.1",
    "@tiptap/extension-image": "^3.11.1",
    "@tiptap/extension-link": "^3.11.1",
    "@tiptap/extension-placeholder": "^3.11.1",
    "@tiptap/pm": "^3.11.1",
    "@tiptap/react": "^3.11.1",
    "@tiptap/starter-kit": "^3.11.1",
    "@types/turndown": "^5.0.6",
    "clsx": "^2.0.0",
    "marked": "^9.0.0",
    "next": "^15.0.0",
    "next-auth": "^5.0.0-beta.25",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sanitize-html": "^2.17.0",
    "tailwind-merge": "^2.6.0",
    "turndown": "^7.2.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/sanitize-html": "^2.16.0",
    "autoprefixer": "^10.4.0",
    "better-sqlite3": "^12.4.6",
    "canvas": "^3.2.0",
    "gif-encoder-2": "^1.0.5",
    "postcss": "^8.4.0",
    "prisma": "^4.16.2",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  }
}
